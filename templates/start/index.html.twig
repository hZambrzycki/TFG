{% apply inline_css %}
<html>
        <head>
            <style>
@import url('https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800,900&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Concert+One&family=Roboto+Slab&display=swap');


.sector{
  display: grid;
  height: 100%;
  place-items: center;
  background: #1B5B85;
}
.container1{
  max-width: 800px;
  height: 430px;
  margin-top: -130px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  padding: 20px;
}
.container1 .box{
    width: 250px;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 20px 30px;
  border-radius: 5px;
  margin:30px;
}
.box .quote i{
margin-top: 10px;
font-size: 45px;
color: #17c0eb
}
.container1 .box .image{
  margin: 10px 0;
  height: 170px;
  width: 170px;
  padding: 3px;
  border-radius: 60%;
}
.box .image img{
  height: 100%;
  width: 100%;
  border-radius: 50%;
  object-fit: cover;
  border: 2px solid #fff;
}
.box p{
  text-align: justify;
  margin-top: 8px;
  font-size: 16px;
  font-weight: 400;
}
.box .name_job{
  margin: 10px 0 3px 0;
  color: #1B5B85;
  font-size: 18px;
  font-weight: 600;
}
.rating i{
  font-size: 18px;
  color: #FCFB86;
  margin-bottom: 5px;
}
.btns{
  margin-top: 20px;
  margin-bottom: 5px;
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.btns button{
  background: #1B5B85;
  width: 100%;
  padding: 9px 0px;
  outline: none;
  border: 2px solid #1B5B85;
  border-radius: 5px;
  cursor: pointer;
  font-size: 18px;
  font-weight: 400;
  color: #1B5B85;
  transition: all 0.3s linear;
}
.btns button:first-child{
  background: none;
  margin-right: 5px;
}
.btns button:last-child{
  color: #fff;
  margin-left: 5px;
}
.btns button:first-child:hover{
  background: #1B5B85;
  color: #fff;
}
.btns button:hover{
  color: #fff;
}

.tmpe{
    color:  #1B5B85;
    font-style: bold;
    font-size: 18px;
    font-family: 'Concert One';

}

.dvlp {
    color:  #1B5B85;
    font-style: bold;
    font-size: 18px;
    font-family: 'Concert One';
}
@media (max-width:1045px){
  .container1 .box{
    width: calc(50% - 10px);
    margin-bottom: 20px;
  }
}
@media (max-width:710px){
  .container1 .box{
    width: 100%;
  }
}  
*{
	margin: 0;
	padding: 0;
	background-origin: border-box;
	font-family: 'Poppins', sans-serif;
	scroll-behavior: smooth;
}

section{
	padding: 100px;
}

/******************************************/
			/* BANNER */
/******************************************/
.banner{
	position: relative;
	min-height: 100vh;
	background-color:black;
	background-size: cover;
	background-position:center center;
	background-attachment: fixed;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.banner h2{
	font-size: 3em;
	color: #fff;
	font-weight: 500;
	line-height: 1.5em;
}

.banner h2 span{
	font-size: 1.5em;
	font-weight: 700;

}

.banner h3{
	font-size: 1.5em;
	color: #fff;
	font-weight: 500;

}

.btn{
	position: relative;
	background: #06487F;
	display: inline-block;
	color: #fff;
	margin-top: 20px;
	padding: 10px 30px;
	font-size: 18px;
	text-transform: uppercase;
	text-decoration: none;
	letter-spacing: 2px;
	font-weight: 500;
}

/******************************************/
				/*HEADER*/
/******************************************/

header{
	position: fixed;
	top:0;
	left:0;
	width: 90%;
	padding: 40px 100px;
	z-index: 1000;
	display: flex;
	justify-content: space-around;
	align-items: center;
	transition: 0.5s;
}
header.sticky{
	background: #fff;
	padding: 20px 100px;
	box-shadow: 0 5px 20px rgba(0,0,0,0.1);
}
header .logo{
	color: #fff;
	font-size: 24px;
	text-transform: uppercase;
	text-decoration: none;
	font-weight: 700;
	letter-spacing: 2px;
}
header.sticky .logo{
	color: #111;
}
header ul{
	position: relative;
	display: flex;
}

header ul li{
	position: relative;
	list-style: none;
}

header ul li a {
	position: relative;
	display: inline-block;
	margin: 0px 15px;
	color: #fff;
	text-decoration: none;
}

header.sticky ul li a{
	color: #111;
}

.heading{
	width: 100%;
	text-align: center;
	margin-bottom: 70px;
	color:#111;

}

.heading h2{
	font-weight: 600;
	font-size: 30px;
}

/******************************************/
/* SOBRE NOSOTROS */ 
/******************************************/
.content{
	display: flex;
	justify-content: space-between;
}

.contentBx{
	padding-right: 30px;
}

.contentBx h3{
 font-size: 24px;
 margin-bottom: 10px;
}

.w50{
min-width: 50%;

}

img{
	max-width: 100%;
}
/******************************************/
			/*SERVICIOS*
/******************************************/
.services{
	background: #111;
}

.heading.white{
	color:#fff;
}

.services .content{
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	flex-direction: row;
}

.services .content .servicesBx{
	padding: 40px 20px;
	background: #222;
	color: #fff;
	max-width: 340px;
	margin: 20px;
	text-align: center;
	transition: 0.5s;


}

.services .content .servicesBx:hover{
	background:#2196f3;
}

.services .content .servicesBx img{
	max-width: 80px;
	filter: invert(1);
}

.services .content .servicesBx h2{
	font-size: 20px;
	font-weight: 600;
}

/******************************************/
			/* PARTNERS */
/******************************************/
.partners .content{
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}

.partners .content .partnersBx{
	 width: 43%;
	 padding: 20px;
}

.partners .content .partnersBx img{
	max-width: 100%;
}





/******************************************/
			/*Responsividad*/
/******************************************/

@media (max-width: 991px)
{
	header,header.sticky{

		padding: 20px 50px;
		z-index: 1000;
	}
	.menu{
		position: fixed;
		top: 75px;
		left: -200%;
		display: block;
		padding: 100px 50px;
		text-align: center; 
		width: 100%;
		height: 100vh;
		background: #fff;
		transition:0.5s;
		z-index: 999;
		border-top: 1px solid rgba(0,0,0,0.2);
	}

	.menu.active{
		left: 0;
	}


	.toggle{
		width: 40px;
		height: 40px;
		background: url(img/menu.png);
		background-position: center;
		background-repeat: no-repeat;
		background-size:30px;
		cursor: pointer;
	}
	.toggle.active{
		
		background: url(img/close.png);
		background-position: center;
		background-repeat: no-repeat;
		background-size:25px;
		cursor: pointer;
	}

	header.sticky .toggle{
		filter: invert(1);
	}
	
	header ul li a{
		color: #111;
		margin:10px;
		font-size: 24px;
	}
	
/* Responsividad Banner*/
.banner{
	padding: 150px 50px 100px;
}

.banner h2{
	font-size: 1.5em;
}

.banner h3{
	font-size: 1em;
}
.btn{
	margin-top: 10px;
	padding: 10px 20px;
	font-size: 16px;
}
.heading h2{
	
	font-size: 24px;
}
/*Responsividad Sobre nosotros */
.contentBx h3{
	font-size: 20px;

}
.content{
	flex-direction: column;
}
.w50{
	margin-bottom: 20px;
}
/*Responsividad servicios*/
.services .content .servicesBx{
	margin: 10px;
}
/*Responsividad partners*/
.partners .content .partnersBx{
	width: 100%;
	padding: 10px;
}




}
</style>
</head>
</html>
{% endapply %}


